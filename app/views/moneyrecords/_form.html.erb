<%= form_for @moneyrecord do |f| %>
  <p>
    <%= f.label :date %>
    <%= f.text_field :date, data:{ provide:'datepicker' } %>
  </p>
  <p>
    <%= f.label :credit %>
    <%= f.check_box :credit %>
  </p>
  <p>
    <%= f.label :desc %>
    <%= f.text_area :desc %>
  </p><p>
    <%= f.label :amount %>
    <%= f.number_field :amount %>
  </p>
  <p>
    <%= f.label :total %>
    <%= f.number_field :total %>
  </p>

  <div class="action">
    <%= f.submit %>
  </div>
<% end %>
